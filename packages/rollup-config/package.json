{
  "name": "@barusu/rollup-config",
  "version": "0.0.1",
  "description": "rollup for react+ts+stylus",
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/barusu.git"
  },
  "homepage": "https://github.com/lemon-clown/barusu/tree/master/scaffold/rollup-config#readme",
  "keywords": [
    "rollup",
    "typescript",
    "stylus"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 12.16.0"
  },
  "files": [
    "lib/",
    ".eslintrc",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "prebuild": "yarn lint:fix && rimraf *.tsbuildinfo lib/",
    "build": "tsc -b tsconfig.json",
    "postbuild": "rimraf lib/package.json lib/test lib/**/*.tsbuildinfo && mv lib/src/* lib && rimraf lib/src/",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/eslint": "^6.1.8",
    "@types/rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup": "^1.31.1",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-typescript2": "^0.26.0"
  },
  "devDependencies": {
    "@barusu/eslint-config": "^0.0.1"
  }
}
