{
  "name": "@barusu/rollup-config",
  "version": "0.0.4",
  "description": "rollup configs for ts project",
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/barusu.git"
  },
  "homepage": "https://github.com/lemon-clown/barusu/tree/master/packages/rollup-config#readme",
  "keywords": [
    "rollup",
    "typescript"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 12.16.0"
  },
  "files": [
    "lib/",
    ".eslintrc.js",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map"
  ],
  "scripts": {
    "prebuild": "yarn lint:fix && rimraf *.tsbuildinfo lib/",
    "build": "tsc -p tsconfig.src.json --sourceMap --declarationMap",
    "postbuild": "rimraf lib/**/*.tsbuildinfo",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prepublishOnly": "yarn prebuild && tsc -p tsconfig.src.json"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/eslint": "^6.1.8",
    "@types/rollup-plugin-peer-deps-external": "^2.2.0",
    "builtin-modules": "^3.1.0",
    "rollup": "^1.31.1",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-typescript2": "^0.26.0"
  },
  "devDependencies": {
    "@barusu/eslint-config": "^0.0.4"
  }
}
