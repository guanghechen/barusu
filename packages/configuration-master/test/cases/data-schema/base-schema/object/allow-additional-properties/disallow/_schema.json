{
  "type": "object",
  "allowAdditionalProperties": false,
  "propertyNames": {
    "type": "string",
    "pattern": "^xx-"
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "age": {
      "type": "number",
      "minimum": 1,
      "maximum": 200,
      "required": false
    }
  }
}
