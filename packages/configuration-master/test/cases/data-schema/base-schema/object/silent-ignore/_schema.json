{
  "type": "object",
  "allowAdditionalProperties": true,
  "silentIgnore": true,
  "propertyNames": {
    "type": "string",
    "pattern": "^(name|age)$"
  },
  "properties": {
    "gender": {
      "type": "string",
      "enum": [ "male", "female" ]
    },
    "name": {
      "type": "string",
      "minLength": 3,
      "maxLength": 25,
      "required": true
    }
  }
}
