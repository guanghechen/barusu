{
  "name": "@barusu/configuration-master",
  "version": "0.0.25",
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/barusu.git"
  },
  "homepage": "https://github.com/lemon-clown/barusu/tree/master/packages/configuration-master#readme",
  "keywords": [
    "json-schema",
    "config util"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 12.16.0"
  },
  "files": [
    "lib/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map"
  ],
  "scripts": {
    "clean": "rimraf test/**/*.output.json test/**/*.outputs.json",
    "start": "rollup -w -c rollup.config.js",
    "prebuild": "yarn test && rimraf lib/",
    "build": "rollup -c rollup.config.js",
    "prepublishOnly": "cross-env ROLLUP_USE_SOURCE_MAP=false yarn build",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "answer": "cross-env node -r ts-node/register test/answer.ts",
    "test": "TS_NODE_FILES=true mocha -r ts-node/register -r tsconfig-paths/register test/**/*.test.ts",
    "test:file": "TS_NODE_FILES=true mocha -r ts-node/register -r tsconfig-paths/register"
  },
  "dependencies": {
    "@barusu/util-option": "^0.0.25"
  },
  "devDependencies": {
    "@barusu/eslint-config": "^0.0.25",
    "@barusu/rollup-config": "^0.0.25",
    "@barusu/util-mocha": "^0.0.25",
    "@types/chai": "^4.2.11",
    "@types/fs-extra": "^8.1.0",
    "@types/mocha": "^8.0.2",
    "chai": "^4.2.0",
    "chai-exclude": "^2.0.2",
    "cross-env": "^7.0.2",
    "fs-extra": "^8.1.0",
    "mocha": "^8.1.1",
    "rollup": "2.16.1"
  }
}
