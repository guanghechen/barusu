{
  "title": "module-statement",
  "cases": [
    {
      "title": "import only",
      "input": "import \"module-name\";"
    },
    {
      "title": "import type",
      "cases": [
        {
          "title": "base",
          "input": "import type { ClassType } from \"module-name\";"
        },
        {
          "title": "import type (space before '{' is optional')",
          "input": "import type{ ClassType } from \"module-name\";"
        }
      ]
    },
    {
      "title": "export type",
      "cases": [
        {
          "title": "base",
          "input": "export type { ClassType } from \"module-name\";"
        },
        {
          "title": "export type (space before '{' is optional')",
          "input": "export type{ ClassType } from \"module-name\";"
        }
      ]
    },
    {
      "title": "default import",
      "cases": [
        {
          "title": "base",
          "input": "import defaultExport from \"module-name\";"
        },
        {
          "title": "+import all 1",
          "input": "import defaultExport, * as name from \"module-name\";"
        },
        {
          "title": "+import all 2",
          "input": "import defaultExport * as name from \"module-name\";"
        },
        {
          "title": "+import all 3",
          "input": "import * as name, defaultExport from \"module-name\";"
        },
        {
          "title": "+import alias",
          "input": "import React, { useEffect, useState as waw } from 'react'"
        }
      ]
    },
    {
      "title": "import alias",
      "cases": [
        {
          "title": "base",
          "input": "import * as name from \"module-name\";"
        }
      ]
    },
    {
      "title": "importN",
      "cases": [
        {
          "title": "base",
          "input": "import { export } from \"module-name\";"
        },
        {
          "title": "+multiple 1",
          "input": "import { export1 , export2 } from \"module-name\";"
        },
        {
          "title": "+multiple 2",
          "input": "import { foo , bar } from \"module-name/path/to/specific/un-exported/file\";"
        },
        {
          "title": "+alias 1",
          "input": "import { export as alias } from \"module-name\";"
        },
        {
          "title": "+alias 2",
          "input": "import { export1 , export2 as alias2, export3 } from \"module-name\";"
        }
      ]
    },
    {
      "title": "multiple line",
      "cases": [
        {
          "title": "base",
          "input": "import { export1, export2, export3 as export3, export4    ,export5, export_long_long_long_long_name } from 'module-name'"
        }
      ]
    }
  ]
}
