{
  "name": "@barusu/tool-git-cipher",
  "version": "0.2.1",
  "description": "A cli tool to cipher git repository",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/barusu.git",
    "directory": "tools/git-cipher"
  },
  "homepage": "https://github.com/guanghechen/barusu/tree/main/tools/git-cipher#readme",
  "keywords": [
    "cipher tool"
  ],
  "main": "lib/cjs/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">=14.15.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "barusu-git-cipher": "./lib/cjs/cli.js"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.cli.js",
    "prebuild": "rimraf lib/ && tsc -p tsconfig.src.json --emitDeclarationOnly",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build"
  },
  "dependencies": {
    "@guanghechen/chalk-logger": "^1.7.0",
    "@guanghechen/event-bus": "^1.7.0",
    "@barusu/util-cli": "^0.2.1",
    "@guanghechen/option-helper": "^1.7.0",
    "@guanghechen/plop-helper": "^1.7.0",
    "@types/fs-extra": "^9.0.11",
    "@types/inquirer": "^7.3.2",
    "command-exists": "^1.2.9",
    "commander": "^7.2.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.4",
    "inquirer": "^8.1.1",
    "node-plop": "~0.26.0"
  }
