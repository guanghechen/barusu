  "dependencies": {
    "@types/fs-extra": "^8.1.0",
    "chalk": "^3.0.0",
    "fs-extra": "^8.1.0",
    "globby": "^11.0.0",
    "is-plain-object": "^3.0.1"
  },
  "devDependencies": {
    "@barusu/eslint-config": "^0.0.22",
    "@barusu/rollup-config": "^0.0.22",
