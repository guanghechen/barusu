{
  "name": "@barusu/rollup-plugin-copy",
  "version": "0.0.41",
  "description": "rollup plugins to copy files and folds (glob supported)",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/barusu.git"
  },
  "homepage": "https://github.com/guanghechen/barusu/tree/master/scaffolds/rollup-plugin-copy#readme",
  "keywords": [
    "rollup",
    "typescript"
  ],
  "main": "lib/cjs/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 12.16.0"
  },
  "files": [
    "lib/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map"
  ],
  "scripts": {
    "prebuild": "rimraf lib/",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "prepublishOnly": "cross-env ROLLUP_USE_SOURCE_MAP=false yarn build",
    "test": "echo 'no test yet.'",
    "lint:fix": "eslint '{src,__test__}/**/*.{ts,tsx}' --fix"
  },
  "dependencies": {
    "@types/fs-extra": "^8.1.0",
    "chalk": "^3.0.0",
    "fs-extra": "^8.1.0",
    "globby": "^11.0.0",
    "is-plain-object": "^3.0.1"
  },
  "devDependencies": {
    "@barusu/eslint-config": "^0.0.41",
    "@barusu/rollup-config": "^0.0.41",
    "rollup": "2.28.1"
  }
}
