{
  "version": "0.0.15",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "publish": {
      "verifyAccess": false,
      "ignoreChanges": [
        "*.md",
        "*.txt",
        "test/**",
        "**/test**/"
      ],
      "message": ":bookmark:  chore(release): publish %s",
      "push": false
    },
    "version": {
      "allowBranch": "master"
    }
  }
}
